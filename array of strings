#include <stdio.h>
#include <string.h>

int main()
{
    char cars[][10] = {"lambo","porche","buggati"};

    strcpy(cars[0], "Tesla"); //to update/edit one of the values/element in the array

    for(int i = 0; i < sizeof(cars)/sizeof(cars[0]); i++)
    {
        printf("%s\n", cars[i]);
    }

    return 0;
}
