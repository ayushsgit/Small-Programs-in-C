#include <stdio.h>

double square(double x) //this x is a local value x , like u can also write 'p' here, it would work the same

    return x * x;       //then here it would be return p * p;

}

int main()
{
    double k;
    printf("print the number u wanna square:\n");
    scanf("%lf", &k);
    double x = square(k);

    printf("sqaure of %lf is %lf ",k ,x);

return 0;

}
